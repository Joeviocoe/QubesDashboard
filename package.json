  "keywords": [
    "Qubes",
    "Dashboard",
    "management",
    "manager"
  ],
  "devDependencies": {
    "electron": "^1.8.2",
    "electron-builder": "^20.1.1"
  },
  "dependencies": {
    "electron-prompt": "^0.5.0",
