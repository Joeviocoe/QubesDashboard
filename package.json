{
  "name": "QubesDashboard",
  "version": "0.1.4",
  "description": "A Graphical Dashboard for the common management of Qubes 4.0",
  "license": "MIT",
  "author": {
    "name": "Joeviocoe",
    "email": "Joeviocoe@electron.com"
  },
  "main": "main.js",
